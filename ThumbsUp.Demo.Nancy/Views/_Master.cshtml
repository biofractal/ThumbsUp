@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title>@ViewBag.Title</title>
	<link href="/Content/bootstrap.min.css" rel="stylesheet" />
	<link href="/Content/thumbsup.demo.nancy.css" rel="stylesheet" />
	@RenderSection("CSS", required: false)
</head>

<body>
    <div class="container">
        <div class="row">
            <div class="navbar navbar-fixed-top">
                <div class="navbar-inner">
                    <div class="container">
                        <a class="brand" href="/"><i class="icon-thumbs-up"></i><strong> ThumbsUp</strong> - Nancy Demo</a>
                        @RenderSection("NavMenus", required: false)
                    </div>
                </div>
            </div>
        </div>
        <div class="row" style="margin-top:100px">
            @RenderBody()
        </div>
    </div>

    @RenderSection("Partial", required: false) 

    <script src="/Scripts/jquery-2.0.3.min.js"></script>
    <script src="/Scripts/jqBootstrapValidation.js"></script>
	<script src="/Scripts/bootstrap.min.js"></script>
	<script src="/Scripts/bootbox.min.js"></script>

	@RenderSection("JS", required: false)

</body>
</html>
