@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>
	@{
	Layout = "_Master.cshtml";
	}
	@section NavMenus{
	<a href="/logout" class="btn btn-primary pull-right"><i class="icon-off icon-white"></i> Sign Out</a>
	}
	<div class="row">
		<h3 class="well">Welcome @Html.GetUser().UserName</h3>
	</div>
	<div class="row">
		<h4>What would you like to do?</h4>
		<ul class="button-container">
			<li><button data-toggle="modal" data-target="#user-create-modal" class="btn"><i class="icon-user"></i> Create New User</button></li>
			<li><button data-toggle="modal" data-target="#application-register-modal" class="btn"><i class="icon-tag"></i> Create New Apppication</button></li>
			<li><button data-toggle="modal" data-target="#application-transfer-modal" class="btn"><i class="icon-tags"></i> Transfer Existing Apppication</button></li>
		</ul>
	</div>
	@section Partial
	{
	@Html.Partial("Partials/UserCreateModal")
	@Html.Partial("Partials/ApplicationRegisterModal")
	@Html.Partial("Partials/ApplicationTransferModal")
	}
	@section JS
	{
	<script src="/Scripts/home.min.js"></script>
	}

